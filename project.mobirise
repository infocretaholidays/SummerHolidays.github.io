            ".timeline-text-content": {
              "padding": "2rem 2.5rem",
              "margin-left": "2rem",
              "text-align": "left",
              "p": {
                "margin-bottom": "0"
              }
            },
            ".time-line-date-content": {
              "margin-right": "2rem",
              "p": {
